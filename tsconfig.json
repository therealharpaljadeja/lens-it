{
    "compilerOptions": {
        "baseUrl": ".",
        "jsx": "preserve",
        "esModuleInterop": true,
        "paths": {
            "@components*": ["components*"],
            "@abis*": ["src/abis*"],
            "@utils*": ["src/utils*"],
            "@lib*": ["src/lib*"],
            "@gql*": ["src/graphql*"],
            "@generated*": ["src/generated*"]
        },
        "target": "es5",
        "lib": ["dom", "dom.iterable", "esnext"],
        "allowJs": true,
        "skipLibCheck": true,
        "strict": false,
        "forceConsistentCasingInFileNames": true,
        "noEmit": true,
        "incremental": true,
        "module": "esnext",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": true
    },
    "include": [
        "next-env.d.ts",
        "**/*.ts",
        "**/*.tsx",
        "context/ApolloContext.jsx"
    ],
    "exclude": ["node_modules"]
}
